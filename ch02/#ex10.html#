  <body>
    <script type="text/javascript">

      var deferred = $.Deferred();
      var promise = deferred.promise();
      
      function succeed(value){
	  return $.Deferred(function(d){
	      d.resolve(value);
	  }).promise();
      }

    </script>
  </body>
